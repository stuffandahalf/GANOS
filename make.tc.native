ARCH=i386
OS=none
EXE_FMT=elf
AS=as
CC=clang
LD=ld
_ASFLAGS=--target=$(ARCH)-$(OS)-$(EXE_FMT) -nostdinc -fuse-ld=$(LD)
ASFLAGS=--target=$(ARCH)-$(OS)-$(EXE_FMT) -nostdinc
_CFLAGS=--target=$(ARCH)-$(OS)-$(EXE_FMT) -nostdinc -fuse-ld=$(LD)
CFLAGS=--target=$(ARCH)-$(OS)-$(EXE_FMT) -nostdinc
_LDFLAGS=--target=$(ARCH)-$(OS)-$(EXE_FMT) -nolibc -fuse-ld=$(LD)
INCLUDE_DIRS=include
